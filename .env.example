# Configurações do Agente TikTok

# Intervalo de monitoramento em segundos (padrão: 300 = 5 minutos)
INTERVALO=300

# Exibir navegador (true para visualizar, false para headless)
HEADLESS=false

# Probabilidades (0.0 a 1.0)
PROBABILIDADE_CURTIDA=0.6
PROBABILIDADE_RESPOSTA=0.4

# Caminho do ChromeDriver
CHROMEDRIVER_PATH=/path/to/chromedriver

# Caminho de saída dos logs
LOG_DIR=./logs
